<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.View">
   <children>
      <VBox alignment="CENTER_RIGHT" spacing="10.0">
         <children>
            <TextField fx:id="userNameTextField" alignment="BOTTOM_RIGHT" promptText="Username" />
            <PasswordField fx:id="passwordTextField" alignment="CENTER_RIGHT" promptText="Password" />
            <Button fx:id="loginButton" mnemonicParsing="false" text="Login" textAlignment="CENTER" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>







